{
	"name": "@danielsharkov/svelte-router-examples",
	"version": "3.0.0",
	"private": false,
	"type": "module",
	"scripts": {
		"example-1": "rollup -c -w --name simple_router -i simple/main.ts",
		"example-2": "rollup -c -w --name advanced_router -i advanced/main.ts",
		"gh-pages": "rollup -c gh-pages.rollup.config.js -w",
		"build": "rollup -c gh-pages.rollup.config.js",
		"start": "sirv public --no-clear --single --host 0.0.0.0",
		"check": "svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@danielsharkov/svelte-router": "^3.1.5",
		"@rollup/plugin-commonjs": "^23.0.2",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-typescript": "^9.0.2",
		"@tsconfig/svelte": "^3.0.0",
		"rollup": "^3.2.5",
		"rollup-plugin-css-only": "^4.3.0",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "~4.8.4"
	},
	"dependencies": {
		"sirv-cli": "^2.0.2"
	}
}
